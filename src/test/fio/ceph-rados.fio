# Runs a 64k random write test against the ceph BlueStore.
[global]
ioengine=libfio_ceph_rados.so # must be found in your LD_LIBRARY_PATH

conf=ceph.conf # must point to a valid ceph configuration file
pool=fio_pool  # must specify existing Ceph pool
stats_print=1  # determines whether Ceph performance counters to be printed on completion

#rw=randread
rw=randwrite
iodepth=16

time_based=1
runtime=10s

[job1]]
nr_files=32
size=4m
bs=64k

